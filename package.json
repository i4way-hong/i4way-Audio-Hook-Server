  "license": "MIT",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "setup": "npm install",
    "start": "ts-node src/index.ts",
    "build": "tsc --project tsconfig.json --pretty && npm run lint",
    "build:ts": "tsc --project tsconfig.json --pretty",
    "buildcheck": "tsc --project tsconfig.json --pretty --noEmit && npm run lint",
    "lint": "eslint . --ext .ts --cache --max-warnings 0",
    "test": "jest",
    "update": "ncu -t minor -u && npm install",
    "clean": "rm -r dist/",
    "sidecar": "ts-node audiohook/src/sidecar/server.ts",
    "sidecar:build": "tsc --project tsconfig.json --pretty && node dist/audiohook/src/sidecar/server.js",
    "sidecar:mock": "ts-node scripts/mrcp-sidecar-mock.ts",
  "build:native": "node-gyp configure build --directory ./audiohook/src/sidecar/signaling/native",
  "send:rtp": "ts-node scripts/send_rtp_pcmu_example.ts"
  },
  "keywords": [],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.297.0",
    "@aws-sdk/client-s3": "3.297.0",
    "@aws-sdk/client-secrets-manager": "3.297.0",
    "@fastify/websocket": "6.0.1",
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "audiohook-fargate": "file:..",
    "dotenv": "16.0.3",
    "fastify": "4.15.0",
    "fastify-plugin": "3.0.1",
    "install": "0.13.0",
    "object-hash": "3.0.0",
    "pino": "8.11.0",
    "pino-pretty": "8.1.0",
    "tdigest": "0.1.2",
    "uuid": "8.3.2",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@aws-sdk/client-transcribe-streaming": "3.529.0",
    "@types/aws-sdk": "2.7.0",
    "@types/bytebuffer": "5.0.44",
    "@types/jest": "28.1.8",
    "@types/node": "16.18.18",
    "@types/object-hash": "2.2.1",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "bytebuffer": "5.0.1",
    "eslint": "8.36.0",
    "node-gyp": "^10.0.1",
    "ts-jest": "28.0.8",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5"
  },
  "optionalDependencies": {
    "node-addon-api": "^7.0.0",
    "node-gyp-build": "^4.8.0"
  }
}
